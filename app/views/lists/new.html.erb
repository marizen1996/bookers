<h1>New book</h1>

<% if @list.errors.any? %>
  <%= @list.errors.count %>errors
  <ul>
    <% @list.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
  </ul>
<% end %>

<%= form_with model: List.new do |f| %>
 <h4>Title</h4>
 <%= f.text_field :title %>

 <h4>Body</h4>
 <%= f.text_area :body %>

 <%= f.submit 'Create Book' %>

 <% end %>


